- if @photos.blank?
  #gallery_no_photos
    =t('gallery.no_photos_in_this_gallery')
    =link_to t('gallery.upload_photos?'), new_user_gallery_photo_path(current_user, @gallery)
- else

  #selected_photo

  #photos

    - @photos.each do |photo|
      = link_to_photo @gallery, photo

:javascript
  $('#photos').find('a')[0].click();